# Pharos Vault Environment Configuration
# Copy this file to .env and fill in your values

# Private key for deployment (without 0x prefix)
# WARNING: Never commit your actual private key!
PRIVATE_KEY=your_private_key_here

# Pharos Testnet RPC URL
PHAROS_TESTNET_RPC_URL=https://testnet.dplabs-internal.com

# Pharos Mainnet RPC URL (for future use)
PHAROS_RPC_URL=https://rpc.pharos.xyz

# Pharos Block Explorer API Key (optional, for contract verification)
PHAROS_API_KEY=your_api_key_here
